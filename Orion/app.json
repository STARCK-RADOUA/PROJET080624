{
  "expo": {
    "name": "Orion",
    "slug": "Orion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/4gVJP0fdoZw00-aLyy3w--transformed.webp",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.france.orion",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This identifier will be used to send notifications.",
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
     
      ],
      "package": "com.france.orion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e7053047-cf1d-400e-b255-1faee969efbb"
      }
    }
  }
}
