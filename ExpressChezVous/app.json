{
  "expo": {
    "name": "ExpressChezVous",
    "slug": "ExpressChezVous",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ExpressChezVous",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "NSCameraUsageDescription": "Cette application a besoin d'accéder à la caméra pour scanner les QR codes.",
        "NSLocationWhenInUseUsageDescription": "This app requires access to your location to provide accurate address services.",
        "NSUserTrackingUsageDescription": "This identifier will be used to deliver personalized ads to you."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.ExpressChezVous",
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
    
      ],
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow ExpressChezVous to access your camera"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "45d377ef-5a79-42d4-8c70-64c57e628942"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/45d377ef-5a79-42d4-8c70-64c57e628942"
    }
  }
}
