{
  "expo": {
    "name": "Nimbus",
    "slug": "Nimbus",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.riderhard.nimbus",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We need your location to provide real-time tracking.",
        "NSLocationAlwaysUsageDescription": "We need your location to provide continuous tracking even when the app is in the background.",
        "UIBackgroundModes": ["location", "fetch", "remote-notification"]


      },
      "runtimeVersion": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "NOTIFICATIONS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "package": "com.riderhard.nimbus",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8a04b1f0-bf32-40a0-8ce1-354efa7c0cf0"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/8a04b1f0-bf32-40a0-8ce1-354efa7c0cf0"
    }
  }
}
